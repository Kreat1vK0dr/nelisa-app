<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Spaza - {{title}}</title>
</head>
<body>
  <div id="title">
      <h1>{{title}}</h1>
</div>
  <table class="table table-striped">
    <!-- <tr> <th colspan="6"> My First HandleBars Table </th> </tr> -->
    <tbody>
    <tr > <td></td> <td>Product/Category</td> <td>Quantity Sold</td> <td>Inventory</td> <td>Profit</td> <td>Profit Margin</td> </tr>
      {{#each data}}
        <tr class="active">
          <td>{{what}}</td> <td >{{item}}</td> <td >{{quantity}}</td> <td >{{inventory}}</td> <td >{{profit}}</td> <td >{{profitMargin}}</td>
        </tr>
        {{/each}}
          </tbody>

  </table>
</body>
</html>

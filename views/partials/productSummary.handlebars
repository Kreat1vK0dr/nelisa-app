      <td class="inverse">{{product}}</td> <td>{{category}}</td> <td class="left-indent">{{quantity}}</td> <td class="left-indent">{{bracketNegative revenue}}</td> <td class="left-indent">{{cost}}</td> <td class="left-indent">{{bracketNegative profit}}</td> <td class="left-indent">{{bracketNegative profitMargin}}</td>
    </tr>
    {{/each}}
  </tbody>

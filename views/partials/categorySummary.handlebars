<tr>
  <th>Category</th>
  <th>Quantity Sold</th>
  <th>Revenue (ZAR)</th>
  <th>Cost (ZAR)</th>
  <th>Profit (ZAR)</th>
  <th>Profit Margin (%)</th>
</tr>
</thead>
<tbody>
  {{#data}}
    <tr>
      <td style="background-color:{{negValRowHL profit}}" class="inverse">{{category}}</td>
      <td style="background-color:{{negValRowHL profit}}" class="left-indent">{{quantity}}</td>
      <td style="background-color:{{negValRowHL profit}}" class="left-indent">{{revenue}}</td>
      <td style="background-color:{{negValRowHL profit}}" class="left-indent">{{cost}}</td>
      <td style="background-color:{{negValRowHL profit}}" class="left-indent">{{profit}}</td>
      <td style="background-color:{{negValRowHL profit}}" class="left-indent">{{profitMargin}}</td>
    </tr>
    {{/data}}
</tbody>
